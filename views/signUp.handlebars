      <input class="form-control form-control-lg" type="text" placeholder="Username" id="username-signup" aria-label=".form-control-lg example"/>
    </div>
    <div class="form-group">
       <input class="form-control form-control-lg" type="password" placeholder="Password" id="password-signup" aria-label=".form-control-lg example"/>
    </div>

    <div class="form-group">
      <button class="btn btn-dark" type="submit">Signup</button>
    </div>
  </form>
  <div>
    <a href="/login">Login Instead</a>
  </div>
</div>
</div>

